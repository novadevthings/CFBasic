10 REM GUESS THE NUMBER GAME
20 PRINT "GUESS THE NUMBER (1-100)"
30 N=INT(RND(1)*100)+1
40 T=0
50 PRINT "ENTER YOUR GUESS:"
60 INPUT G
70 T=T+1
80 IF G=N THEN GOTO 130
90 IF G<N THEN PRINT "TOO LOW!"
100 IF G>N THEN PRINT "TOO HIGH!"
110 GOTO 50
130 PRINT "CORRECT! YOU GOT IT IN"; T; "TRIES!"
140 END
